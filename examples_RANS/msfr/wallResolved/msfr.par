#Nek5000 parameter file
[GENERAL]
startFrom = msfr.fld
stopAt = numSteps
numSteps = 10000
dt = 5.0e-5
writeInterval = 10000
timeStepper = BDF2

[PROBLEMTYPE]
equation = incompNS
axiSymmetry = yes
stressFormulation = yes
variableProperties = yes

[MESH]
writeToFieldFile = yes

[VELOCITY]
density = 1.0
viscosity = -40000.0
residualTol = 1e-8
residualPROJ = yes

[PRESSURE]
residualTol = 1e-6
residualPROJ = yes

[TEMPERATURE]
solver = none

[SCALAR01] 
density = 1.0
diffusivity = -40000.0
residualTol = 1e-8

[SCALAR02] 
density = 1.0
diffusivity = -40000.0
residualTol = 1e-8
